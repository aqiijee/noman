@{
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}
@model IEnumerable<EAD_TermProject.Models.item>
@using EAD_TermProject.Models

<!DOCTYPE html>
<html>
    <head>
        <title></title>
        <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/jquery.js"></script>



    </head>
    <body>
        <div class="container">
         <table class="table table-bordered">
        <tr class="info">
           <th>ItemId</th><th>CategoryId</th><th>ItemName</th><th>BrandName</th><th>ItemPrice</th><th>ItemSize</th><th>ItemImageName</th>
        </tr>
    @foreach(var data in Model)
    {
        <tr class="success">
            <td><big>@data.itemid</big></td>
            <td><big>@data.categoryid</big></td>
            <td><big>@data.itemname</big></td>
            <td><big>@data.brandname</big></td>
            <td><big>@data.itemprice</big></td>
            <td><big>@data.itemsize</big></td>
            <td><big>@data.itemimage</big></td>

            <td><a href="/Admin/delete1/@data.itemid">Delete</a></td>
            @{
        Session["idd"] = data.itemid;
                }
            <td><a href="/Admin/update">Update</a></td>
        </tr>
    }
        </table>
            </div>



    </body>
</html>

